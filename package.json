{
  "name": "cloudflare",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore ./src/**/*",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore --ignore-unknown --no-error-on-unmatched-pattern --check .",
    "prettier:fix": "npm run prettier -- --write",
    "type-check": "tsc --noEmit"
  },
  "prettier": "@lshay/prettier-config",
  "eslintConfig": {
    "extends": "@lshay/eslint-config",
    "rules": {
      "no-new": "off"
    }
  },
  "dependencies": {
    "@pulumi/cloudflare": "^4.9.0",
    "@pulumi/pulumi": "^3.0.0"
  },
  "devDependencies": {
    "@lshay/eslint-config": "^0.1.0",
    "@lshay/prettier-config": "^0.5.0",
    "@types/node": "^14",
    "eslint": "^8.22.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2"
  }
}
